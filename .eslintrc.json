{
  "extends": "next/core-web-vitals",
  "rules": {
    "quotes": ["error", "single"],
    "semi": ["error", "always"],
    "no-console": 1,
    "no-unused-vars": 1,
    "no-irregular-whitespace": 1,
    "no-trailing-spaces": 1,
    "no-const-assign": 1,
    "no-duplicate-imports": 1,
    "camelcase": 1
  }
}
